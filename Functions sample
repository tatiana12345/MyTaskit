
public class Functions1 {

	public static void main(String[] args) {
		//FUNCTION PRACTICE:
		//1. String
		myFunction("Anna");
		
		//2. String+Integer
		myFunction2("David",23);
		
		//3. Integer Sum of few values
		int total = myFunction3(3,5,7);
		System.out.println(total);
		
		//4. Call function4 and pass FName, LName
		String names = myFunction4("Anna", "Smith");
		System.out.println(names);
		
		//5. No input, only output
		String value = myFunction5();
		System.out.println(value);
		
		//System output, no values passed
		System.out.println("Hi Sophie!01");
		System.out.println("Hi Sophie!03");

	}
//5. No input, only output	
   private static String myFunction5() {
		return "No input output, Output only";
		
	}

//4. Call function4 and pass FName, LName
    private static String myFunction4(String FName, String LName) {
	String NoSpaces = FName + LName;
		return NoSpaces;
	}
//3. Integer / Pass / Return a value/ output values SUM	
    private static int myFunction3(int x1, int x2, int x3) {
	int SUM = x1 + x2 + x3;
	return SUM;
		
	}
    
//2. Integer Pass a Value to the variable
    private static void myFunction2(String name_Male, int age) {
	System.out.println("Hello " + name_Male +" (" + age + ")");	
	
	}

//1. Define the function-myFunction
	private static void myFunction(String name_Female) {
		System.out.println("Hi Sophie!" +name_Female);
		System.out.println(name_Female);
		
		
		
		
