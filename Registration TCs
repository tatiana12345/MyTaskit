import org.openqa.selenium.WebDriver;
import org.openqa.selenium.firefox.FirefoxDriver;
import org.openqa.selenium.By;

public class Registration {

	public static void main(String[] args) throws InterruptedException {
		// TODO Auto-generated method stub
		//Launch URL
  
///CONSUMER REG.
		
	    WebDriver wd = new FirefoxDriver();
	    wd.get("http://qa.app.mytaskit.com"); 
	    
	    //Click SignUp as a consumer
	    wd.findElement(By.id("btnRegister")).click();
	    wd.findElement(By.id("personalTab")).click();
	    
	    //Type 1st, last , display name 
	    wd.findElement(By.id("FirstNameOwner")).sendKeys(new String [] {"consumer"});
	    wd.findElement(By.id("LastNameOwner")).sendKeys(new String[] {"last"});
	    wd.findElement(By.id("DisplayNameOwner")).sendKeys(new String [] {"selenium_test"});
	    
	    //Type in email and confirm email
	    wd.findElement(By.id("EmailOwner")).sendKeys(new String[] {"selenium5@yahoo.com"});
	    wd.findElement(By.id("EmailConfirmOwner")).sendKeys(new String[] {"selenium5@yahoo.com"});
	    
	    
	    //Type in Password and confirm password
	    wd.findElement(By.id("PasswordOwner")).sendKeys(new String [] {"passowrd"});
	    wd.findElement(By.id("PasswordConfirmOwner")).sendKeys(new String [] {"passowrd"});
	    
	    
	    //Type a zip code
	    wd.findElement(By.id("ZipCodeOwner")).sendKeys(new String [] {"33480"});
	    
	    //Uncheck im a boat Owner
	    wd.findElement(By.id("IsBoatOwner")).click();
	     
	    //Check terms of use
	    wd.findElement(By.id("termsOfUse")).click();
	    
	    //Click SignUp
	    wd.findElement(By.id("btnSaveData")).click();
	    
	    Thread.sleep(5000);
	    
	    //Close pop-up
	    wd.findElement(By.xpath("//i[@class= 'glyphicon glyphicon-remove-circle']")).click();
	    
	    //WAIT AND Logout Consumer
	    Thread.sleep(3000);
	    wd.findElement(By.id("navTopRightMenu")).click();
	    Thread.sleep(8000);
	    wd.findElement(By.xpath("//ul/li/a[contains(text(),'Logout')]")).click();
